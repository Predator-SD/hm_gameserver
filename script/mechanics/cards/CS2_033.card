# Water Elemental
{
    ".section attack" : {
        "code" : "
            hsl_attack($attacker, $defenders);
        ",

        "target" : "$minions"
    },

    ".section attack_effect" : {
        "code" : "
            hsl_freeze($attacker, $defender)
        "
    },

    ".section properties" : {
        "all": [ "freeze" ]
    }
}

